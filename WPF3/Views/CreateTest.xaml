<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF3.Views"
             xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" x:Class="WPF3.Views.CreateTest"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ListBox SelectedItem="{Binding SelectedLeft}" ItemsSource="{Binding QuestionListFrom}" Margin="76,48,435,48"/>
        <Button Command="{Binding CommandLoad}" Content="Button" HorizontalAlignment="Left" Margin="23,225,0,0" VerticalAlignment="Top"/>
        <ListBox SelectedItem="{Binding SelectedRight}" ItemsSource="{Binding QuestionListTo}" Margin="390,48,108,48"/>
        <Button Command="{Binding MoveRightCommand}" Content=">>>" HorizontalAlignment="Left" Margin="336,407,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.328,0.544"/>
        <Button Command="{Binding MoveLeftCommand}" Content="----" HorizontalAlignment="Left" Margin="390,407,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
